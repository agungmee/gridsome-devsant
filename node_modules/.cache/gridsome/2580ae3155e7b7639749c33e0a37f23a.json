            <h2 class=\"title is-5\">About {{ $page.author.title }}</h2>\n                <p>{{ $page.author.blurb }}</p>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </section>\n  </Layout>\n</template>\n\n<page-query>\n query($id: ID!) {\n    author(id: $id) {\n      title\n      path\n      image\n      blurb\n      content\n      belongsTo {\n        totalCount\n        pageInfo {\n          totalPages\n          currentPage\n        }\n        edges {\n          node {\n            ... on Post {\n              id\n              title\n              excerpt\n              featuredImage\n              path\n              content\n              date(format:\"MMMM Do YYYY\")\n              \n          \n              author {\n                id\n                title\n                image\n                path\n              }\n            }\n          }\n        }\n      }\n    }  \n  }\n</page-query>\n<script>\nimport PostItem from '../components/PostItem';\nexport default {\n  components: { PostItem },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n@import '@/assets/scss/overrides.scss';\n\n.post {\n  padding-bottom: 1rem;\n  border-bottom: 12px solid $primary;\n}\n</style>\n"]}]}